<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:Mirrack.ViewModels"
			 xmlns:controls="using:Mirrack.Controls"
             mc:Ignorable="d" d:DesignWidth="240" d:DesignHeight="256"
             x:Class="Mirrack.Views.WeatherModule.IconView"
			 x:DataType="vm:WeatherModule.IconViewModel">

	<controls:LCorner>
		<Border  Background="Black" Width="240">
			<controls:WeatherWidget Icon="{Binding Icon}" Summary="{Binding Summary}" Temperature="{Binding Temperature}" TemperatureHigh="{Binding TemperatureHigh}" TemperatureLow="{Binding TemperatureLow}" PrecipProbability="{Binding PrecipProbability}" WindSpeed="{Binding WindSpeed}" />
		</Border>
	</controls:LCorner>
</UserControl>
